{
  "name": "react-library-paginator",
  "version": "1.0.1",
  "description": "React bootstrap-oriented customizable paginator component",
  "homepage": "https://github.com/margaretkrutikova/react-library-paginator#readme",
  "author": "MargaretKrutikova",
  "license": "MIT",
  "repository": "margaretkrutikova/react-library-paginator",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom --watchAll",
    "test:snapshot-update": "react-scripts-ts test --env=jsdom -u",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "@types/classnames": "^2.2.6",
    "classnames": "^2.2.6"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "gh-pages": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts-ts": "^2.17.0",
    "react-test-renderer": "^16.4.2",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss-modules": "^1.0.8",
    "rollup-plugin-typescript2": "^0.16.1",
    "rollup-plugin-url": "^1.4.0",
    "typescript": "^3.0.1"
  },
  "files": [
    "dist"
  ]
}
